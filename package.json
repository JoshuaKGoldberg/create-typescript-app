{
	"author": "Josh Goldberg <npm@joshuakgoldberg.com>",
	"description": "Template TypeScript Node.js package with all the CI bells & whistles I commonly use. âœ¨",
	"devDependencies": {
		"@semantic-release/changelog": "^6.0.1",
		"@semantic-release/commit-analyzer": "^9.0.2",
		"@semantic-release/git": "^10.0.1",
		"@semantic-release/github": "^8.0.6",
		"@semantic-release/npm": "^9.0.1",
		"@semantic-release/release-notes-generator": "^10.0.3",
		"@typescript-eslint/eslint-plugin": "^5.38.1",
		"@typescript-eslint/parser": "^5.38.1",
		"cspell": "^6.12.0",
		"eslint": "^8.24.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-simple-import-sort": "^8.0.0",
		"husky": ">=6",
		"lint-staged": ">=10",
		"markdownlint-cli": "^0.32.2",
		"prettier": "^2.7.1",
		"semantic-release": "^19.0.5",
		"typescript": "^4.8.4",
		"vitest": "^0.23.4"
	},
	"license": "MIT",
	"lint-staged": {
		"*": "prettier --ignore-unknown --write"
	},
	"name": "template-typescript-node-package",
	"scripts": {
		"build": "tsc",
		"format:write": "yarn format --write",
		"format": "prettier \"**/*\" --ignore-unknown",
		"lint:md": "npx markdownlint \"**/*.md\" \".github/**/*.md\"",
		"lint:spelling": "cspell \"**\" \".github/**/*\"",
		"lint": "npx eslint . --max-warnings 0 --report-unused-disable-directives",
		"prepare": "husky install",
		"test": "vitest"
	},
	"type": "module",
	"version": "1.1.0"
}
