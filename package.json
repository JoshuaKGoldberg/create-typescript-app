{
	"name": "create-typescript-app",
	"version": "1.73.3",
	"description": "Quickstart-friendly TypeScript template with comprehensive, configurable, opinionated tooling. 💝",
	"repository": {
		"type": "git",
		"url": "https://github.com/JoshuaKGoldberg/create-typescript-app"
	},
	"license": "MIT",
	"author": {
		"name": "Josh Goldberg ✨",
		"email": "npm@joshuakgoldberg.com"
	},
	"type": "module",
	"main": "./lib/index.js",
	"bin": "./bin/index.js",
	"files": [
		"bin/index.js",
		"lib/",
		"package.json",
		"LICENSE.md",
		"README.md"
	],
	"scripts": {
		"build": "tsup",
		"format": "prettier .",
		"initialize": "tsx ./bin/index.js --mode initialize",
		"lint": "eslint . --max-warnings 0",
		"lint:knip": "knip",
		"lint:md": "markdownlint \"**/*.md\" \".github/**/*.md\" --rules sentences-per-line",
		"lint:packages": "pnpm dedupe --check",
		"lint:spelling": "cspell \"**\" \".github/**/*\"",
		"prepare": "husky",
		"test": "vitest",
		"test:create": "npx tsx script/create-test-e2e.ts",
		"test:initialize": "npx tsx script/initialize-test-e2e.ts",
		"test:migrate": "vitest run -r script/",
		"tsc": "tsc"
	},
	"lint-staged": {
		"*": "prettier --ignore-unknown --write"
	},
	"dependencies": {
		"@clack/prompts": "^0.7.0",
		"all-contributors-for-repository": "^0.3.0",
		"chalk": "^5.3.0",
		"execa": "^9.3.1",
		"get-github-auth-token": "^0.1.0",
		"git-remote-origin-url": "^4.0.0",
		"git-url-parse": "^15.0.0",
		"js-yaml": "^4.1.0",
		"lazy-value": "^3.0.0",
		"npm-user": "^6.1.1",
		"octokit": "^4.0.2",
		"parse-author": "^2.0.0",
		"prettier": "^3.3.3",
		"replace-in-file": "^8.1.0",
		"rimraf": "^6.0.1",
		"title-case": "^4.3.1",
		"zod": "^3.23.8",
		"zod-validation-error": "^3.3.1"
	},
	"devDependencies": {
		"@eslint-community/eslint-plugin-eslint-comments": "^4.4.0",
		"@eslint/js": "^9.9.0",
		"@octokit/request-error": "^6.1.4",
		"@release-it/conventional-changelog": "^9.0.0",
		"@types/eslint-plugin-markdown": "^2.0.2",
		"@types/eslint__js": "^8.42.3",
		"@types/git-url-parse": "^9.0.3",
		"@types/js-yaml": "^4.0.9",
		"@types/node": "^22.3.0",
		"@types/parse-author": "^2.0.3",
		"@vitest/coverage-v8": "^2.1.0",
		"@vitest/eslint-plugin": "^1.1.0",
		"c8": "^10.1.2",
		"console-fail-test": "^0.5.0",
		"cspell": "^8.14.2",
		"eslint": "^9.9.0",
		"eslint-plugin-jsdoc": "^50.4.0",
		"eslint-plugin-jsonc": "^2.16.0",
		"eslint-plugin-markdown": "^5.1.0",
		"eslint-plugin-n": "^17.11.0",
		"eslint-plugin-package-json": "^0.15.2",
		"eslint-plugin-perfectionist": "^3.2.0",
		"eslint-plugin-regexp": "^2.6.0",
		"eslint-plugin-yml": "^1.14.0",
		"globby": "^14.0.2",
		"husky": "^9.1.4",
		"jsonc-eslint-parser": "^2.4.0",
		"knip": "5.33.1",
		"lint-staged": "^15.2.9",
		"markdownlint": "^0.35.0",
		"markdownlint-cli": "^0.42.0",
		"prettier-plugin-curly": "^0.3.0",
		"prettier-plugin-packagejson": "^2.5.1",
		"prettier-plugin-sh": "^0.14.0",
		"release-it": "^17.6.0",
		"sentences-per-line": "^0.2.1",
		"tsup": "^8.3.0",
		"tsx": "^4.17.0",
		"typescript": "^5.6.2",
		"typescript-eslint": "^8.1.0",
		"vitest": "^2.1.0"
	},
	"engines": {
		"node": ">=18.3.0"
	},
	"publishConfig": {
		"provenance": true
	}
}
